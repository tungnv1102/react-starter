{
  "name": "webpack-react-redux-babel-sass-starter",
  "version": "0.2.0",
  "description": "Webpack + ReactJS + Redux + Babel + SASS",
  "main": "app/index.jsx",
  "scripts": {
    "sass-lint": "sass-lint -c ./sass-lint.yml app/**/**.scss",
    "dist": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack -p",
    "dev": "node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --hot",
    "test": "standard --fix --verbose | snazzy",
    "watch": "node -r babel-register ./node_modules/karma/bin/karma start --singleRun false"
  },
  "author": "HieuNV",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hieunv8/webpack-react-redux-babel-sass-starter.git"
  },
  "bugs": {
    "url": "https://github.com/hieunv8/webpack-react-redux-babel-sass-starter/issues"
  },
  "sasslintConfig": "./sass-lint.yml",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.20.0",
    "babel-runtime": "^6.23.0",
    "immutable": "^3.8.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "isparta-loader": "^2.0.0",
    "jasmine-ajax": "^3.3.1",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "node-sass": "^4.1.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "redux-logger": "^2.7.4",
    "redux-mock-store": "^1.2.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  }
}
