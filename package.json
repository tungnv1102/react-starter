{
  "name": "webpack-react-redux-babel-sass-starter",
  "version": "0.0.0",
  "description": "Webpack + ReactJS + Redux + Babel + SASS",
  "main": "app/index.jsx",
  "scripts": {
    "sass-lint": "sass-lint -c ./sass-lint.yml app/**/**.scss",
    "eslint": "eslint app/**/*.js app/**/*.jsx app/*.js app/*.jsx test/*.js test/*.jsx test/**/*.js test/**/*.jsx",
    "lint": "npm run eslint && npm run sass-lint",
    "dist": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack -p",
    "dev": "node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --hot",
    "test": "node -r babel-register ./node_modules/karma/bin/karma start --singleRun true",
    "watch": "node -r babel-register ./node_modules/karma/bin/karma start --singleRun false"
  },
  "author": "HieuNV",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hieunv8/webpack-react-redux-babel-sass-starter.git"
  },
  "bugs": {
    "url": "https://github.com/hieunv8/webpack-react-redux-babel-sass-starter/issues"
  },
  "sasslintConfig": "./sass-lint.yml",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.0",
    "bootstrap-sass": "^3.3.7",
    "cross-env": "^3.1.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.0",
    "eslint-plugin-react": "^6.3.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "immutable": "^3.8.1",
    "isparta-loader": "^2.0.0",
    "jasmine-ajax": "^3.2.0",
    "jasmine-core": "^2.5.2",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.10.1",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.2.1",
    "redux-promise-middleware": "^4.0.0",
    "redux-thunk": "^2.1.0",
    "requirejs": "^2.3.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  }
}
