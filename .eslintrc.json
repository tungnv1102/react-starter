{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "installedESLint": true,
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": [
            1,
            {
                "args": "all",
                "argsIgnorePattern": "^React"
            }
        ],
        "no-undef": [
            1
        ]
    }
}